<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.studentauth.dao.CourseDao">
    <select id="queryCourse" resultType="com.example.studentauth.entity.course">
        SELECT
        course_id,course_name,course_grade
        FROM tb_course
        ORDER BY
        course_grade DESC
    </select>
</mapper>