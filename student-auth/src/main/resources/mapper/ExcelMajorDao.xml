<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.studentauth.dao.ExcelMajorDao">
    <select id="queryExcelMajor" resultType="com.example.studentauth.entity.major">
        SELECT
        major_id,major_name
        FROM tb_major
        ORDER BY
        major_id DESC
    </select>
</mapper>